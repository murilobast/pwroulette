.header
	height 78px
	background-color #333
	&__content
		max-width 960px
		margin auto
		&__brand, &__links
			display inline-block
			line-height 78px
		&__brand
			&__title
				margin 0
				color #FFF
				font-weight normal
		&__links
			height 24px 
			line-height 24px  
			margin 0 20px 
			&__link
				display inline-block
				a
					padding 4px 12px
					color #FFF
					text-decoration none